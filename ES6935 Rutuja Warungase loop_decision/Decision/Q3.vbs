  Q3 Print the character is vowel or consonants

dim s
 s= inputbox("Enter the character")
 
select case s
 case "a"
  msgbox("vowel")

case "i"
  msgbox("vowel")

case "o"
  msgbox("vowel")

case "e"
  msgbox("vowel")

case "u"
  msgbox("vowel")

case else
  msgbox("Consonants")

end select



